/**
 * @param {string} s
 * @param {string} t
 * @return {boolean}
 */
var isAnagram = function(s, t) {
    if(s.length !== t.length) return false;
    let charCountS = {};
    for(i = 0; i < s.length; i++){
        !charCountS[s[i]] ? charCountS[s[i]] = 1 : charCountS[s[i]] += 1;
    }

    for(j = 0; j < t.length; j++){
        if(!charCountS[t[j]] || charCountS[t[j]] === 0){
            return false;
        } else {
            charCountS[t[j]] -= 1;
        }
    }

    return true;
};